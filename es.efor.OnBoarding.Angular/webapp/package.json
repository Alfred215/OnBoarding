{
  "name": "GestorActividades-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "postinstall": "ngcc",
    "build": "npm run build:dev",
    "build:dev": "./node_modules/.bin/ng build --watch",
    "build:dev-nowatch-stats": "ng build --stats-json",
    "build:prod": "npm run build:prod-aot",
    "build:prod-aot": "ng build --prod",
    "build:prod-jit": "ng build --prod --aot=false",
    "build:prod-jit-stats": "ng build --prod --aot=false --buildOptimizer=false --stats-json",
    "build:prod-aot-stats": "ng build --prod --stats-json",
    "swagger": "ng-openapi-gen --input http://localhost:5000/swagger/v1/swagger.json --output src/app/shared/api",
    "translate": "ngx-translate-extract --input ./src --output ./src/assets/i18n/es.json --format namespaced-json --key-as-default-value --sort",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "npm-run-all build:dev-nowatch-stats analyze",
    "stats:prod": "npm-run-all build:prod-aot-stats analyze",
    "analyze": "webpack-bundle-analyzer ../../es.efor.GestorActividades.MainGateway/wwwroot/stats.json",
    "favicon": "start https://realfavicongenerator.net"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/localize": "^11.1.0",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@angular/service-worker": "~11.0.3",
    "@auth0/angular-jwt": "^5.0.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@coreui/angular": "~2.11.1",
    "@coreui/coreui": "^2.1.16",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.3.1",
    "@coreui/icons": "^2.0.0-rc.0",
    "@coreui/icons-angular": "1.0.0-alpha.3",
    "@fortawesome/angular-fontawesome": "~0.7.0",
    "@fortawesome/fontawesome-svg-core": "~1.2.31",
    "@fortawesome/free-brands-svg-icons": "~5.15.0",
    "@fortawesome/free-regular-svg-icons": "~5.15.0",
    "@fortawesome/free-solid-svg-icons": "~5.15.0",
    "@fullcalendar/angular": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@ngx-translate/core": "~13.0.0",
    "@ngx-translate/http-loader": "~6.0.0",
    "ax-toolbox": "0.0.50",
    "bootstrap": "~4.5.2",
    "chart.js": "^2.9.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.8.3",
    "file-saver": "^2.0.5",
    "flag-icon-css": "^3.5.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-dist": "^1.12.1",
    "jquery.fullscreen": "^0.6.0",
    "moment": "^2.29.0",
    "ng-lazyload-image": "^8.0.1",
    "ng2-charts": "^2.4.2",
    "ngx-bootstrap": "~6.1.0",
    "ngx-device-detector": "^1.5.2",
    "ngx-perfect-scrollbar": "^10.1.0",
    "ngx-toastr": "~13.0.1",
    "rxjs": "~6.6.0",
    "simple-line-icons": "^2.5.5",
    "ts-helpers": "^1.1.2",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "xlsx": "^0.16.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-openapi-gen": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
